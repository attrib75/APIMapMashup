<!DOCTYPE html>
<html lang="en">
<head>
    <title>Bing and Azure Maps Mashup!</title>
    <!-- Meta -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Bing and Azure Maps Mashup by Patrick Goode">
    <meta name="author" content="Patrick Goode">
    <link rel="shortcut icon" href="favicon.ico">

    <script src="~/lib/jquery/dist/jquery-3.5.0.min.js"></script>
    <script src="https://code.jquery.com/ui/1.9.2/jquery-ui.js"></script>

    <link href='https://fonts.googleapis.com/css?family=Lato:300,400,300italic,400italic' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>

    <!-- FontAwesome JS -->

    <script src="~/lib/fontawesome/js/all.js"></script>

    <!-- Global CSS -->

    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />

    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/octicons/2.0.2/octicons.min.css">

    <!-- Theme CSS -->
    <link href="~/css/styles.css" rel="stylesheet" />
</head>

<body>
    <!-- ******HEADER****** -->
    <header class="header">
        <div class="container clearfix">

            <img class="profile-image img-fluid float-left" style="width: 225px; margin-right: 10px; transform: scale(0.8);"
                 src="~/images/placeholder.com-logo4.png" alt="Company Placeholder Image" />

            <div class="profile-content float-left">

                <h2 class="desc">A Bing and Azure Maps Mashup!</h2>

                <ul class="social list-inline">
                    <li><i class="fas fa-map-marker-alt"></i>&nbsp;&nbsp;<span class="sr-only">Location:</span>Restaurants in Chicago, &nbsp;Illinois</li>
                </ul>
            </div><!--//profile-->
        </div><!--//container-->
    </header><!--//header-->

    @RenderBody()
    @RenderSection("Scripts", required: false)

    <!-- ******FOOTER****** -->
    <footer class="footer">
        <div class="container text-center">
            <!--/* This template is free as long as you keep the attribution link below. Thank you for your support. :) If you'd like to use the template without the attribution, you can buy the commercial license via our website: themes.3rdwavemedia.com */-->
            <small class="copyright">Designed with <i class="fas fa-heart"></i> by <a href="https://themes.3rdwavemedia.com" target="_blank">Xiaoying Riley</a> for developers</small>
        </div><!--//container-->
    </footer><!--//footer-->
</body>
</html>